// THEME CSS
html,
body{
	color: #43458B;
}
.toolbar-btns{
	margin-left: 10px;
}
.sidebar{
	width 300px;
    background-color: #17141F;
    color: #93909B;
}
.sidebar .text-primary{
    color: #93909B !important;
}
.sidebar .q-item-letter,
.sidebar .q-item-icon{
	font-size:24px;
}
.sidebar .q-list-link > .q-item:hover{
    background-color: #0E0C13;
}

.sidebar .logo{
	padding: 10px;
	padding-top: 15px;
	padding-left: 20px;
    height: 50px;
    text-align: left;
    border-radius: 0;
    font-weight: 500;
    background-color: #43458B;
    color: #fff;
}
.sidebar .sidebar-list{
	-webkit-box-shadow: inset 0px 5px 5px -5px rgba(0,0,0,0.5);
	-moz-box-shadow: inset 0px 5px 5px -5px rgba(0,0,0,0.5);
	box-shadow: inset 0px 5px 5px -5px rgba(0,0,0,0.5);
}

//OTHERS
.q-layout-page{
	-webkit-box-shadow: inset 0px 5px 5px -5px rgba(0,0,0,0.5);
	-moz-box-shadow: inset 0px 5px 5px -5px rgba(0,0,0,0.5);
	box-shadow: inset 0px 5px 5px -5px rgba(0,0,0,0.5);
	background-color: #FAF9FA
}

.q-btn .q-icon, 
.q-btn .q-spinner
	font-size 1em
.q-toolbar
	padding-left 15px
	padding-right 15px
.row-input
	margin-bottom 10px